        "import json\n",
        "import unicodedata\n",
        "\n",
        "def limpar_texto(texto):\n",
        "    # 1. Normaliza para decompor caracteres (ex: Ã§ -> c + ,)\n",
        "    texto_normalizado = unicodedata.normalize('NFD', texto)\n",
        "\n",
        "    # 2. Remove acentos (diacrÃ­ticos)\n",
        "    texto_sem_acento = \"\".join([c for c in texto_normalizado if not unicodedata.combining(c)])\n",
        "\n",
        "    # 3. Remove Emojis comuns (abordagem simples com replace)\n",
        "    # Como o Gemini pode enviar qualquer emoji, Ã© mais difÃ­cil listar todos.\n",
        "    # O mÃ©todo abaixo Ã© eficiente se vocÃª jÃ¡ souber o emoji ou\n",
        "    # preferir converter o texto para ASCII e remover o que nÃ£o for letra/nÃºmero.\n",
        "\n",
        "    # Exemplo: texto_final = texto_sem_acento.replace('ðŸ˜Š', '').replace('ðŸš€', '')\n",
        "\n",
        "    # Melhor abordagem: encoder/decoder ASCII (remove caracteres nÃ£o ASCII)\n",
        "    texto_limpo = texto_sem_acento.encode('ascii', 'ignore').decode('ascii')\n",
        "\n",
        "    return texto_limpo\n",
        "\n",
        "for user in users:\n",
        "  user['news'] = limpar_texto(user['news'])\n",
        "\n",
        "with open('Resultado.json', 'w', encoding='utf-8') as arquivo_json:\n",
        "    # 3. Usa json.dump() para escrever os dados no arquivo\n",
        "    # indent=4 para formatar, ensure_ascii=False para acentos\n",
        "    json.dump(users, arquivo_json, indent=4, ensure_ascii=False)\n",
        "\n",
        "print(json.dumps(users, indent = 2))\n"
